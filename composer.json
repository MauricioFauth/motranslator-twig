{
    "name": "phpmyadmin/motranslator-twig",
    "description": "Twig intergration for translation API for PHP using Gettext MO files",
    "license": "GPL-2.0-or-later",
    "keywords": ["gettext", "mo", "translator", "i18n"],
    "homepage": "https://github.com/phpmyadmin/motranslator-twig",
    "authors": [
        {
            "name": "The phpMyAdmin Team",
            "email": "developers@phpmyadmin.net",
            "homepage": "https://www.phpmyadmin.net/team/"
        }
    ],
    "support": {
        "issues": "https://github.com/phpmyadmin/motranslator-twig/issues",
        "source": "https://github.com/phpmyadmin/motranslator-twig"
    },
    "require": {
        "php": ">=5.3.0",
        "symfony/expression-language": "^4.0 || ^3.2 || ^2.8"
    },
    "require-dev": {
        "sami/sami": "^4.0",
        "phpunit/php-code-coverage": "*",
        "phpunit/phpunit": "~4.8 || ~5.7 || ~6.5"
    },
    "autoload": {
        "psr-4": {
            "PhpMyAdmin\\MoTranslator\\Twig\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpMyAdmin\\MoTranslator\\Twig\\Tests\\": "tests"
        }
    }
}
